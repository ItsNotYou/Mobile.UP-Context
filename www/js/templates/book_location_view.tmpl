<% _.each(locations, function(location) { %>
<div>
	<h3><%= location.get('department') %></h3>
	<span class="available"></span> <!--Alternativen: lent, prebook -->
	<%= (location.get('label')) ? "<p>"+location.get('label')+"<br>" : "" %>
	<a href="<%= location.get('url') %>"><%= location.get('availableitems') %></a></p>
</div>
<% }); %>