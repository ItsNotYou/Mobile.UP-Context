<h2>Results</h2>
<ul data-role="listview" data-inset="true">
    <% _.each(booklist, function(book) { %>
    <li id="<%= book.get('recordId') %>">
        <a href="#">
            <h2><%= book.get('title') %></h2>
            <p><%= book.get('subtitle') %></p>
        </a>
    </li>
    <% }); %>
</ul>

<a href="#loadNext" data-role="button" class="pagination-button">Mehr Ergebnisse laden</a>
