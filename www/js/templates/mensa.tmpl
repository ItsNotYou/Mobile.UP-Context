<% _.each(meals, function(meal) { %>
<div data-role="collapsible" data-collapsed="false" id="<%= meal.contentId %>">
    <h3>
        <div class="mensaicons">
            <img src="<%= meal.ingredients[0] %>" />
        </div>
        <div class="mensatext"><%= meal.title %><br /><span><%=meal.description %></span></div>
	</h3>
	<p>
		[3] mit Antioxidationsmitteln<br />
		Studierende: <%= meal.prices.students %> €<br />
		Bedienstete: <%= meal.prices.staff %> €<br />
		Gäste: <%= meal.prices.guests %> €
	</p>
</div>
<% }); %>