<!-- Start Header -->
<div data-role="header" data-position="fixed" data-tap-toggle="false">
	<a href="#nav-panel" data-role="button" data-icon="false" data-inline="true" class="menubutton">Menue</a>
	<a href="#campus" data-role="button" data-icon="false" data-inline="true" class="back">Back</a>
	<h1>Notrufnummern
	<%  if(store.get('up.session.authenticated')){ %>
        <span class="username-header nooptions"><%= store.get('up.session.username') %></span><a href="#options" class="settings" data-role="button" data-icon="logout-blue" title="logout" alt="logout"></a></h1>
    <%  }else{ %>
        <a href="#options" class="settings" data-role="button" data-icon="login-blue" title="login" alt="login"></a></h1>
    <%  } %>
</div>
<!-- End Header -->
<!-- Start Main - Navigation -->
<div data-role="content" data-iconpos="left" data-theme="c"
	id="emergencies">
	<form>
		<input data-type="search" id="searchForEmergency"
			placeholder="Eintr&auml;ge filtern" />
	</form>
	<ul data-role="listview" data-inset="true" data-icon="false"
		id="emergency-list" data-filter="true"
		data-input="#searchForEmergency">
	</ul>
</div>
<!-- End Main - Navigation -->